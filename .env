# Spendex Environment Configuration
# Copy this file to .env.development and .env.production

# API Configuration
API_BASE_URL=https://api.spendex.in/api/v1

# Razorpay Keys (replace with actual keys)
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=

# Firebase Configuration (add your firebase config)
# FIREBASE_PROJECT_ID=
# FIREBASE_API_KEY=
# FIREBASE_APP_ID=

# Feature Flags
ENABLE_BIOMETRIC=true
ENABLE_VOICE_ENTRY=true
ENABLE_RECEIPT_SCAN=true
ENABLE_ANALYTICS=true

# Debug
DEBUG_MODE=true
